{
	"editor.formatOnSave": false,
	"eslint.validate": ["vue", "javascript", "typescript"],
	"eslint.packageManager": "npm",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
	},
	"files.exclude": {
		"**/.git": true,
		".gitignore": true,
		"**/.DS_Store": true,
		"package-lock.json": true,
		"node_modules": true,
		"dist": true,
		".vite": true,
		".output": true,
		".nuxt": true,
		"**/tsconfig.json": true,
		"nuxt.config.ts": true,
		".eslintrc": true,
		".vscode": true,
		"**/dist/": true,
		"vite.config.js": true,
		"build/": true,
		"src/renderer/types/": true,
		"scripts": true,
		"electron-builder.json": true,
	},
}